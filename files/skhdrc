# move window
shift + cmd - a : yabai -m window --warp west
shift + cmd - s : yabai -m window --warp south
shift + cmd - w : yabai -m window --warp north
shift + cmd - d : yabai -m window --warp east

# focus window
shift + alt - a : yabai -m window --focus west
shift + alt - s : yabai -m window --focus south
shift + alt - w : yabai -m window --focus north
shift + alt - d : yabai -m window --focus east

# toggle window split type
alt - e : yabai -m window --toggle split

# float / unfloat window and center on screen
alt - t : yabai -m window --toggle float;\
          yabai -m window --grid 4:4:1:1:2:2

# change layout of desktop
ctrl + alt - a : yabai -m space --layout bsp
ctrl + alt - d : yabai -m space --layout float          